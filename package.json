{
  "name": "beer-sdk",
  "version": "0.0.1",
  "description": "wrapper packages most used for backend services",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint",
    "test": "jest --watch --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hunandika/beer-sdk.git"
  },
  "keywords": [
    "sdk"
  ],
  "author": "hunandikaadyota",
  "email": "hunandikaadyota@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hunandika/beer-sdk/issues"
  },
  "homepage": "https://github.com/hunandika/beer-sdk#readme",
  "_moduleAliases": {
    "@src": "src",
    "@config": "src/config",
    "@logger": "src/logger/logger",
    "@cache": "src/cache/cacheManager",
    "@pJson": "package.json"
  },
  "jest": {
    "moduleNameMapper": {
      "@src": "<rootDir>/src",
      "@config": "<rootDir>/src/config",
      "@logger": "<rootDir>/src/logger/logger",
      "@cache": "<rootDir>/src/cache/cacheManager",
      "@pJson": "<rootDir>/package.json"
    }
  },
  "devDependencies": {
    "del": "^6.0.0",
    "eslint": "^7.23.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1"
  },
  "dependencies": {
    "bunyan": "^1.8.15",
    "bunyan-debug-stream": "^2.0.0",
    "bunyan-slack": "^0.0.10",
    "cache-manager": "^3.4.3",
    "cache-manager-mongodb": "git+https://github.com/hunandika/node-cache-manager-mongodb.git",
    "cache-manager-redis-store": "^2.0.0",
    "dotenv": "^8.2.0",
    "install": "^0.13.0",
    "module-alias": "^2.2.2"
  }
}
